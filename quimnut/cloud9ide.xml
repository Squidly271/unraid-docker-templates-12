<?xml version="1.0" encoding="utf-8"?>
<Containers>
<Beta>False</Beta>
<Category>Development:IDE</Category>
  <Name>cloud9ide</Name>
  <Description>
    cloud9 is a web IDE from https://c9.io/ [br][br]
    [b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
    [b]/workspace[/b] This is the cloud9 workspace, map this locally[br]
    [b]C9USER and C9PASS[/b] Turn on advanced settings to see this, it is the http basic auth credentials for the app[br][br]
  </Description>
  <Overview>
    cloud9 is a web IDE from https://c9.io/ [br][br]
  </Overview>
  <Support>http://lime-technology.com/forum/index.php?topic=</Support>
  <Registry>https://hub.docker.com/r/quimnut/cloud9ide/</Registry>
  <GitHub>https://github.com/quimnut/unraid-docker-templates/quimnut/</GitHub>
  <Repository>quimnut/cloud9ide</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8064</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>C9USER</Name>
      <Value>webuser</Value>
    </Variable>
    <Variable>
      <Name>C9PASS</Name>
      <Value>cloud9</Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>path to cloud9 workspace</HostDir>
      <ContainerDir>/workspace</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8064]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/quimnut/unraid-docker-templates/master/quimnut/c9icon.png</Icon>
</Containers>
